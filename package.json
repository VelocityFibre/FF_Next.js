{
  "name": "fibreflow-react",
  "version": "1.0.0",
  "description": "FibreFlow React Migration - Modern React application migrated from Angular",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "next dev",
    "dev:api": "node server.mjs",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "BUNDLE_ANALYZE=true next build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:smoke": "playwright test --grep @smoke",
    "test:e2e:visual": "playwright test --grep @visual",
    "test:e2e:mobile": "playwright test --grep @mobile",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:strict": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "db:migrate": "tsx scripts/migrations/run.ts",
    "db:seed": "tsx scripts/setupNeonDatabase.ts seed",
    "db:validate": "tsx scripts/setupNeonDatabase.ts validate",
    "db:setup": "tsx scripts/setupNeonDatabase.ts setup",
    "db:test": "tsx scripts/run-database-tests.ts",
    "db:test:suite": "tsx scripts/database-testing-suite.ts",
    "db:test:schema": "tsx scripts/schema-validation-suite.ts",
    "antihall": "node scripts/antihall-validator.cjs",
    "antihall:parse": "node scripts/antihall-validator.cjs parse",
    "antihall:check": "node scripts/antihall-validator.cjs check",
    "antihall:find": "node scripts/antihall-validator.cjs find",
    "antihall:stats": "node scripts/antihall-validator.cjs stats",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "check:db-connections": "node scripts/check-db-connections.js",
    "test:integration": "vitest run src/tests/api-integration",
    "test:no-direct-db": "vitest run src/tests/api-integration/no-direct-db.test.ts",
    "test:logger": "tsx scripts/test-logger.ts",
    "db:migrate:realtime": "node scripts/run-realtime-migration.js"
  },
  "keywords": [
    "react",
    "typescript",
    "fibreflow",
    "migration",
    "nextjs"
  ],
  "author": "FibreFlow Team",
  "license": "MIT",
  "dependencies": {
    "@clerk/nextjs": "^5.7.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@neondatabase/serverless": "^1.0.1",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.85.5",
    "@types/formidable": "^3.4.5",
    "@types/uuid": "^10.0.0",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.5.0",
    "chalk": "^5.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "firebase": "^12.1.0",
    "formidable": "^3.5.4",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.279.0",
    "next": "14.2.18",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "pino": "^9.9.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.15.0",
    "recharts": "^3.1.2",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^0.34.0",
    "@vitest/ui": "^0.34.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.2",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^22.1.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.17.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "^5.2.0",
    "vitest": "^0.34.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
